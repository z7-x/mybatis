<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mybatis.mapper.PermissionMapper">

    <select id="findPermissionByPermissionName" resultType="com.example.mybatis.pojo.Permission">
        select * from mybatis_permission where permission_name = #{permissionName}
    </select>

</mapper>